# cd aliases
alias dv="cd ~/dev"
alias lrn="cd ~/learn"
alias qt="cd ~/Qt"
alias njs="cd ~/Applications/n"
alias brw="cd /usr/local/Cellar"
alias map="cd ~/Applications"
alias nvc="cd ~/.config/nvim"
alias nvs="cd ~/.local/share/nvim"
alias zsh="cd  ~/.config/zsh"
alias dot="cd ~/.dotfiles"
alias cf="cd ~/.config"

# git aliases
alias gst="git status"
alias ga="git add"
alias gc="git commit"
alias gca='git commit -am'
alias gps="git push"
alias gpl="git pull"
alias gl="git log --stat" # recent commits showing changed files
alias gr="git reset HEAD~"  # undo the most recent commit
alias gd="git diff"

# shell aliases
alias la="ls -al"
alias cl="clear"

# launch aliases
alias nv="nvim"
alias pip="pip3"
